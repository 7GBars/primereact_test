{
  "compilerOptions": {
    "target": "es6",
    "module": "commonjs",
    "outDir": "./dist",
    "rootDir": "./src",
    "strict": true,
    "esModuleInterop": true,
    "experimentalDecorators": true,
    "emitDecoratorMetadata": true,
    "jsx": "react-jsx",

    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,


    "paths": {
      "primereact/*": ["node_modules/primereact/*"]
    }
  },
  "include": [
    "src"
  ]
}
